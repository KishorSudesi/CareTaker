<resources>

    <!--
        Base application theme, dependent on API level. This theme is replaced
        by AppBaseTheme from res/values-vXX/styles.xml on newer devices.
    -->
    <style name="AppBaseTheme" parent="Theme.AppCompat.Light.DarkActionBar">

    </style>

    <!-- Application theme. -->
    <style name="AppTheme" parent="AppBaseTheme">
        <!-- All customizations that are NOT specific to a particular API-level can go here. -->
    </style>

    <style name="FormLabel">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#005d9b</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
    </style>

    <style name="FormLabelBlack">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#000000</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
    </style>

    <style name="GridDoc">
        <item name="android:textSize">16dp</item>
        <item name="android:layout_height">24dp</item>
    </style>

    <style name="QABlack">
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#000000</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
    </style>

    <style name="HeaderLabel">
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#FFFFFF</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
    </style>

    <style name="SwitchLabel">
        <item name="android:textSize">20dp</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
    </style>

    <style name="forgotword">
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#005e9e</item>
        <item name="android:textStyle">normal</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
    </style>

    <style name="normaltext">
        <item name="android:textSize">22dp</item>
        <item name="android:textColor">#005e9e</item>
        <item name="android:textStyle">normal</item>
        <!-- <item name = "android:fontFamily">verdana</item> -->
        <item name="android:layout_marginLeft">25dp</item>
    </style>

    <style name="boldtext">
        <item name="android:textSize">22dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#005d9b</item>
    </style>

    <style name="boldtextred">
        <item name="android:textSize">22dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#af2220</item>
    </style>

    <style name="boldtext_fr">
        <item name="android:textSize">26dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#005d9b</item>
    </style>

    <style name="boldtextred_mang">
        <item name="android:textSize">26dp</item>
        <item name="android:textStyle">bold</item>
        <item name="android:textColor">#af2220</item>
    </style>

    <style name="accepted">
        <item name="android:textSize">22dp</item>
        <item name="android:textStyle">bold</item>
    </style>

    <style name="mySeekBar">
        <item name="android:layout_marginRight">4dp</item>
    </style>

    <style name="mySeekBarTerm"></style>

    <style name="lettertext">
        <item name="android:textSize">20dp</item>
        <item name="android:textColor">#ffffff</item>
        <item name="android:textStyle">bold</item>
    </style>
</resources>
